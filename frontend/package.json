  "private": true,
  "engines": {
    "node": "^10.20.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint . --ext .js,.ts"
    ]
  },
  "scripts": {
    "clean": "rm -rf dist",
    "start": "webpack-dev-server --config webpack.config.ts",
    "build:prod": "NODE_OPTIONS=\"--max-old-space-size=8192\" ENV=prodcution webpack --config webpack.config.ts --vervose",
    "deploy": "./deploy.sh",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "test:unit": "NODE_ENV=test jest -u --silent=false",
    "test:unit:watch": "yarn test:unit --watch --verbose",
    "gopherjs": "cd gopherjs && gopherjs build -v -o ../src/libs/cosmos/Amino.js ."
  },
